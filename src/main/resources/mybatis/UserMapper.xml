<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.seproject.mapper.UserMapper">
<!--    这里完成mapper中声明的方法的实现-->
    <select id="selectUserByPhone" resultType="CommonUser">
        select *
        from user
        where userPhone = #{param1};
    </select>

</mapper>